{
  "name": "dravencms/timeline",
  "type": "library",
  "description": "Timeline module for dravencms",
  "keywords": [
    "dravencms",
    "timeline"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
  },
  "require-dev": {
    "nette/tester": "@dev"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": {
        "extensions": {
          "translation": "Kdyby\\Translation\\DI\\TranslationExtension"
        }
      },
      "scripts": {
        "pre-install": "Dravencms\\Timeline\\Scripts::prePackageInstall",
        "post-install": "Dravencms\\Timeline\\Scripts::postPackageInstall"
      }
    }
  }
}